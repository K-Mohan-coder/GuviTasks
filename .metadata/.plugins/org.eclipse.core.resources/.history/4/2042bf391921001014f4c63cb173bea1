package Task_no_3;

public class Library {
	private static Book bookStorage[];
	static int count;
	
    public Library() {
    	bookStorage = new Book[5];
        count = 0;
    }
	
	public static void addBook(Book newbook) {
		bookStorage[count] = newbook;
		System.out.println("Added : "+bookStorage[0].title);
	}
	
	public void display() {

	}
		
}
